<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="670.0" prefWidth="1028.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.MainPageController">
   <children>
      <Label layoutX="573.0" layoutY="60.0" text="欢迎">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <Label fx:id="localUserName" layoutX="621.0" layoutY="92.0" text="Label">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <Label layoutX="671.0" layoutY="124.0" text="进入项目经费管理系统界面！">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <Button fx:id="change_info_button" layoutX="601.0" layoutY="186.0" mnemonicParsing="false" onAction="#changeUserInfo" text="更改用户信息">
         <font>
            <Font size="22.0" />
         </font></Button>
      <Button fx:id="apply_item_button" layoutX="604.0" layoutY="259.0" mnemonicParsing="false" onAction="#toAddItemPage" text="申请项目">
         <font>
            <Font size="22.0" />
         </font></Button>
      <Button fx:id="end_item_button" layoutX="604.0" layoutY="328.0" mnemonicParsing="false" onAction="#toEndItemPage" text="结算项目">
         <font>
            <Font size="22.0" />
         </font></Button>
      <Button fx:id="item_info_button" layoutX="601.0" layoutY="400.0" mnemonicParsing="false" onAction="#toCheckItemPage" text="查看项目信息">
         <font>
            <Font size="22.0" />
         </font>
      </Button>
      <TableView fx:id="itemInfo" prefHeight="670.0" prefWidth="535.0">
        <columns>
          <TableColumn fx:id="id" prefWidth="98.0" text="项目编号" />
          <TableColumn fx:id="itemName" prefWidth="174.0" text="项目名称" />
            <TableColumn fx:id="itemLeader" prefWidth="110.0" text="项目负责人" />
            <TableColumn fx:id="itemFund" prefWidth="152.0" text="项目经费" />
        </columns>
      </TableView>
   </children>
</AnchorPane>
